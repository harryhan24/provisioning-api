{
  "name": "uq-provisioning-api",
  "version": "1.0.0",
  "main": "src/index.ts",
  "license": "MIT",
  "scripts": {
    "deploy": "sls deploy --stage dev",
    "deploy:production": "sls deploy --stage production",
    "deploy:stage": "sls deploy --stage stage",
    "serve": "serverless offline start --port 4000 --dontPrintOutput",
    "test": "jest --watch --coverage"
  },
  "devDependencies": {
    "@types/body-parser": "^1.16.7",
    "@types/cookie-parser": "^1.4.1",
    "@types/express": "^4.0.39",
    "@types/jest": "^21.1.5",
    "@types/ldapjs": "^1.0.1",
    "cross-env": "^5.1.1",
    "jest": "^21.2.1",
    "jest-fetch-mock": "^1.3.3",
    "prettier": "^1.7.4",
    "reflect-metadata": "^0.1.10",
    "sequelize-typescript": "^0.5.0",
    "serverless": "^1.5.1",
    "serverless-offline": "^3.16.0",
    "serverless-plugin-typescript": "^1.1.3",
    "sqlite3": "^3.1.13",
    "superagent": "^3.8.0",
    "supertest": "^3.0.0",
    "ts-jest": "^21.1.4",
    "typescript": "^2.6.1"
  },
  "dependencies": {
    "@types/supertest": "^2.0.3",
    "body-parser": "^1.18.2",
    "cookie-parser": "^1.4.3",
    "express": "^4.16.2",
    "jsonwebtoken": "^8.1.0",
    "ldapjs": "^1.0.1",
    "mysql2": "^1.4.2",
    "saml2-js": "^1.12.2",
    "sequelize": "^4.20.3",
    "serverless-http": "^1.5.2",
    "winston": "^2.4.0",
    "winston-aws-cloudwatch": "^1.6.0"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "./node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "mapCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/config/*",
      "!src/database/**/*",
      "!src/index.ts",
      "!src/utils/logger.ts"
    ],
    "setupFiles": [
      "./src/setupJest.ts"
    ]
  }
}
